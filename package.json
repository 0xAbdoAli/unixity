  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx tsc && jasmine",
    "dev": "nodemon ./src/app.ts",
    "clean": "rimraf build/",
    "build": "yarn clean && npx tsc",
    "start": "npm run build && nodemon build/app.js",
    "migrate-test:down": "db-migrate -e test reset",
    "migrate-test:up": "db-migrate -e test up",
    "migrate-dev:down": "db-migrate -e dev reset",
    "migrate-dev:up": "db-migrate -e dev up",
    "migrate-prod:down": "db-migrate -e prod reset",
    "migrate-prod:up": "db-migrate -e prod up",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "Unixity Chat",
    "unixty chat",
    "Unixity",
    "unixity"
  ],
  "author": [
    "Eslam Mohamed Moawed ELabd",
    "Abdelrahman Ali AbdElHafiz"
  ],
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "socket.io": "^4.5.4",
    "supertest": "^6.3.3",
    "unixityelite-ws": "^1.0.8",
    "ws": "^8.11.0"
