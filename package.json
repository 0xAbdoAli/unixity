    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.16",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuidv4": "^5.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "eslint": "^8.30.0",
